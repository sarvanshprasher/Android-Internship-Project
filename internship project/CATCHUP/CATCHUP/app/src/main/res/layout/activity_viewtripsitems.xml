<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:background="@drawable/mypic"
    android:layout_height="match_parent"
    tools:context="com.example.shivam.catchup.viewtripsitems">

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageView12"
        android:src="@drawable/imageee"
        android:layout_centerHorizontal="true" />
    <View
        android:layout_width="match_parent"
        android:layout_height="2dp"
        android:layout_below="@+id/imageView12"
        android:background="#000000"
        android:layout_marginTop="20dp"
        android:id="@+id/view4" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TRIP NAME:"
        android:textStyle="bold"
        android:layout_marginTop="10dp"
        android:id="@+id/textView2"
        android:layout_alignTop="@+id/view4"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="START DATE:"
        android:textStyle="bold"
        android:layout_marginTop="10dp"
        android:id="@+id/textView20"
        android:layout_below="@+id/textView2"
        android:layout_alignStart="@+id/textView2" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="RETURN DATE:"
        android:layout_marginTop="10dp"
        android:textStyle="bold"
        android:id="@+id/textView22"
        android:layout_below="@+id/textView20"
        android:layout_alignStart="@+id/textView20" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="DETAILS:"
        android:layout_marginTop="10dp"
        android:textStyle="bold"
        android:id="@+id/textView24"
        android:layout_below="@+id/textView22"
        android:layout_alignStart="@+id/textView22" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="EXPENSE:"
        android:textStyle="bold"
        android:id="@+id/textView26"
        android:layout_marginTop="10dp"
        android:layout_below="@+id/textView24"
        android:layout_alignStart="@+id/textView24" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="EVENTS:"
        android:textStyle="bold"
        android:id="@+id/textView28"
        android:layout_marginTop="10dp"
        android:layout_below="@+id/textView26"
        android:layout_alignStart="@+id/textView26" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="PLACES LIKED:"
        android:textStyle="bold"
        android:layout_marginTop="10dp"
        android:id="@+id/textView30"
        android:layout_below="@+id/textView28"
        android:layout_alignStart="@+id/textView28" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="PLACES UNLIKED:"
        android:id="@+id/textView32"
        android:textStyle="bold"
        android:layout_marginTop="10dp"
        android:layout_below="@+id/textView30"
        android:layout_alignStart="@+id/textView30" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="USER_ID"
        android:textStyle="bold"
        android:id="@+id/textView34"
        android:layout_above="@+id/textView21"
        android:layout_alignEnd="@+id/imageView12" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="New Text"
        android:layout_marginTop="10dp"
        android:id="@+id/textView35"
        android:layout_alignBottom="@+id/view4"
        android:layout_toEndOf="@+id/textView34" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/editText20"
        android:layout_above="@+id/textView20"
        android:layout_toEndOf="@+id/textView32" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/editText22"
        android:layout_above="@+id/textView22"
        android:layout_alignStart="@+id/editText20" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/editText23"
        android:layout_alignBottom="@+id/textView22"
        android:layout_alignStart="@+id/editText22" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/editText24"
        android:layout_above="@+id/textView26"
        android:layout_alignStart="@+id/editText23" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/editText25"
        android:layout_alignBottom="@+id/textView26"
        android:layout_alignStart="@+id/editText24" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/editText26"
        android:layout_alignBottom="@+id/textView28"
        android:layout_alignStart="@+id/editText25" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/editText27"
        android:layout_alignBottom="@+id/textView30"
        android:layout_alignStart="@+id/editText26" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/editText28"
        android:layout_alignBottom="@+id/textView32"
        android:layout_toEndOf="@+id/textView32" />

</RelativeLayout>
